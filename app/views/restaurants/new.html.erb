<%= link_to "New Restaurant", "/restaurants/new" %>
<center><%= form_with url: '/restaurants', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :allows_children %>
  <%= form.text_field :allows_children %>
  <%= form.label :rating %>
  <%= form.text_field :rating %>
  
  <%= form.submit 'Create Restaurant' %>
<% end %></center>